{
  "manifest_version": 3,
  "name": "DeleteMixList",
  "version": "0.1",
  "description": "YouTubeのURLからミックスリスト部分を削除してリダイレクトします。",
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "DeleteMixList"
  }, 
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.youtube.com/*"
  ],

  "author": "yamada"

}
